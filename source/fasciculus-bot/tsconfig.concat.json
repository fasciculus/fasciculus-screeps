{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "newLine": "crlf",
    "noImplicitReturns": true,
    "outDir": "dist",
    "rootDir": "concat",
    "strict": true,
    "strictPropertyInitialization": true,
    "target": "ESNext"
  },
  "include": [
    "concat/main.ts",
    "concat/es/types.ts"
  ]
}